x2 := 5;
x2 := x2 + 1;
LOOP x2 DO 
  x0 := x0 + 1
END // -> 6

//---------------------------------------------------
// t(0) = 0, t(1) = 1, t(2) = 4, t(3) = 10, t(4) = 20,
LOOP x1 DO
  x5 := x5 + 1;
  x6 := 0; x4 := 0;
  LOOP x5 DO
    x4 := x4 + 1;
    x3 := 0; x2 := 0;
    LOOP x4 DO
      x2 := x2 + 1;
      x3 := x3 + x2
    END; // x3 = d(x4 )
    x6 := x6 + x3
  END; // x6 = t(x5 )
  IF x6 = x1 THEN x0 := 1 END
END

